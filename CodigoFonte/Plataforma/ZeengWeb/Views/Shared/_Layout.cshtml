@using ZeengWeb.Utils
@{ 
    string DataHora = DateTime.Now.ToString("ddMMyyyyHHmmss");
}
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <title>ZEENG | @ViewBag.Title</title>
    <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700' rel='stylesheet' type='text/css'>
    <link rel="icon" href="~/Images/favicon.png" type="image/x-icon" />

    <!-- Add local styles, mostly for plugins css file -->
    @if (IsSectionDefined("Styles"))
    {
        @RenderSection("Styles", required: false)
    }

    <!-- Add jQuery Style direct - used for jQGrid plugin -->
    <link href="@Url.Content("~/Scripts/plugins/jquery-ui/jquery-ui.css")" rel="stylesheet" type="text/css" />

    <!-- Primary Inspinia style -->
    @*@Styles.Render("~/Content/css")*@
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/animate.css" rel="stylesheet" />
    <link href="~/Content/style.css?@DataHora" rel="stylesheet" />

    @*@Styles.Render("~/font-awesome/css")*@
    <link href="~/fonts/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
</head>
<body class="boxed-layout mini-navbar">
 <input type="hidden" id="iduser" name="iduser" value="@Html.Raw(Configuracoes.idClienteLogado)" />
    @*@if (Session["logado"] != "true")
        {
            Response.Redirect("/login");
        }
        else
        {
            Session["logado"] = "true";
        }*@

    <!-- Wrapper-->
    <!-- PageClass give you ability to specify custom style for specific view based on action -->
    <div id="wrapper" class="@Html.PageClass()">

        <!-- Navigation -->
        @Html.Partial("_Navigation")

        <!-- Page wraper -->
        <div id="page-wrapper" class="gray-bg @ViewBag.SpecialClass">

            <!-- Top Navbar -->
            @Html.Partial("_TopNavbar")

            <!-- Main view  -->
            @RenderBody()

            <!-- Footer -->
            @Html.Partial("_Footer")

           

        </div>
        <!-- End page wrapper-->
        <!-- Right Sidebar -->
        @*@Html.Partial("_RightSidebar")*@
     
       
    </div>

   
    <!-- Footer -->
    @Html.Partial("_FormularioContato")


    <!-- analytics.google -->
    <script>

      var userId = '@Html.Raw(Configuracoes.loginUsuarioLogado)';


      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

      ga('create', 'UA-90674459-1', 'auto');
      ga('set', 'dimension2', userId);
      ga('send', 'pageview');
    </script>
    <!-- End analytics.google -->

    <!-- End wrapper-->
    <!-- Section for main scripts render -->
    @*@Scripts.Render("~/bundles/jquery")*@
    <script src="~/Scripts/jquery-2.1.1.min.js"></script>
    @*@Scripts.Render("~/bundles/bootstrap")*@
    <script src="~/Scripts/bootstrap.min.js"></script>
    @*@Scripts.Render("~/plugins/slimScroll")*@
    <script src="~/Scripts/plugins/slimscroll/jquery.slimscroll.min.js"></script>
    @*@Scripts.Render("~/bundles/inspinia")*@
    <script src="~/Scripts/plugins/metisMenu/metisMenu.min.js"></script>
    <script src="~/Scripts/plugins/pace/pace.min.js"></script>
    <script src="~/Scripts/app/inspinia.js"></script>
    <!-- Skin config script - only for demo purpose-->
        @*@Scripts.Render("~/bundles/skinConfig")*@
        <script src="~/Scripts/app/skin.config.min.js"></script>

    <!-- Handler for local scripts -->
    @RenderSection("scripts", required: false)

    <!-- mascara para cobrir o site -->
    <div class="background-modal"></div>

</body>
</html>
