        </div>

        <div class="col-sm-8">
            <div style="float:left; border: 0px solid red; margin: -3px 0 0 -20px;  padding-top: 10px; min-height: 115px;">
                @{

                    bool active = false;
                    string activeFace = "",
                    activeTw = "",
                    activeInsta = "",
                    activeYou = "";


                    // Facebook
                    if (Model.temRedesSocias.dadosFacebook != null)
                    {
                        active = true;
                        activeFace = "active";

                        string linkface = Model.temRedesSocias.dadosFacebook.Url.Replace("https://www.", "").Replace("http://www.", "").Replace("www.", "").Replace("https://", "").Split('?')[0];

                        <p><a class="iconesRedeSocial" href="@String.Concat("http://",linkface)" target="_blank"><i class="fa @Model.temRedesSocias.dadosFacebook.Icone"></i> @Html.Raw(linkface) </a></p>
                    }
                    else
                    {
                        <p onclick="showFormularioContato(this)"><i class="fa fa-facebook" style="font-size: 16px;"> </i> A empresa não possui perfil monitorado no Facebook &nbsp;|&nbsp; <a class="iconesRedeSocial">Clique aqui para informar</a></p>
                    }

                    // Twitter
                    if (Model.temRedesSocias.dadosTwitter != null)
                    {

                        if (active == false)
                        {
                            active = true;
                            activeTw = "active";
                        }

                        string linktwitter = Model.temRedesSocias.dadosTwitter.Url.Replace("https://www.", "").Replace("http://www.", "").Replace("www.", "").Replace("https://", "").Split('?')[0];

                        <p><a class="iconesRedeSocial" href="@String.Concat("http://",linktwitter)" target="_blank"><i class="fa @Model.temRedesSocias.dadosTwitter.Icone"></i> @Html.Raw(linktwitter)</a></p>
                    }
                    else
                    {
                        <p onclick="showFormularioContato(this)"><i class="fa fa-twitter" style="font-size: 16px;"> </i> A empresa não possui perfil monitorado no Twitter &nbsp;|&nbsp; <a class="iconesRedeSocial">Clique aqui para informar</a></p>
                    }


                    if (Model.temRedesSocias.dadosInstagram != null)
                    {
                        if (active == false)
                        {
                            active = true;
                            activeInsta = "active";
                        }
                        string linkinsta = Model.temRedesSocias.dadosInstagram.Url.Replace("https://www.", "").Replace("http://www.", "").Replace("www.", "").Replace("https://", "").Split('?')[0];

                        <p><a class="iconesRedeSocial" href="@String.Concat("http://",linkinsta)" target="_blank"><i class="fa @Model.temRedesSocias.dadosInstagram.Icone"></i> @Html.Raw(linkinsta) </a></p>
                    }
                    else
                    {
                        <p onclick="showFormularioContato(this)"><i class="fa fa-instagram" style="font-size: 16px;"> </i> A empresa não possui perfil monitorado no Instagram &nbsp;|&nbsp; <a class="iconesRedeSocial">Clique aqui para informar</a></p>
                    }

                    if (Model.temRedesSocias.dadosYoutube != null)
                    {
                        if (active == false)
                        {
                            active = true;
                            activeYou = "active";
                        }

                        string linkyt = Model.temRedesSocias.dadosYoutube.Url.Replace("https://www.", "").Replace("http://www.", "").Replace("www.", "").Replace("https://", "").Split('?')[0];

                        <p><a class="iconesRedeSocial" href="@String.Concat("http://", linkyt)" t target="_blank"><i class="fa @Model.temRedesSocias.dadosYoutube.Icone"></i> @Html.Raw(linkyt)</a></p>
                    }
                    else
                    {
                        <p onclick="showFormularioContato(this)"><i class="fa fa-youtube-play" s style="font-size: 16px;"> </i> A empresa não possui perfil monitorado no Youtube &nbsp;|&nbsp; <a class="iconesRedeSocial">Clique aqui para informar</a></p>
                    }


                }

            </div>
        </div>
