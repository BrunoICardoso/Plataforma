                        @if (Model.TotalSeguidoresFace != null)
                        {
                            <div class="total_curtidas">
                                <span class="indicador">
                                    @if (Model.TotalSeguidoresFace != null)
                                    {
                                        @Html.Raw(String.Format("{0:n0}", Model.TotalSeguidoresFace.totalAtual));
                                    }
                                    else
                                    {
                                        @Html.Raw("0");
                                    }
                                </span><br />
                                <span>
                                    total de fãs
                                </span>
                            </div>
                            <div class="percentual_curtidas">

                                @if (Math.Round(Model.TotalSeguidoresFace.variacao, 2) < 0)
                                {
                                    <i class="fa fa-arrow-down indicador text-danger"></i>
                                }
                                else if (Math.Round(Model.TotalSeguidoresFace.variacao, 2) > 0)
                                {
                                    <i class="fa fa-arrow-up indicador text-navy"></i>
                                }
                                else
                                {
                                    <i class="fa fa-square indicador text-success"></i>
                                }
                                @Html.Raw(String.Format("{0:0.00}", Model.TotalSeguidoresFace.variacao) + "%")
                            </div>
                        }
                        else
                        {
                            <div class="total_curtidas">
